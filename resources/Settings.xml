<Settings>
    <PropertyList>
        <!-- Joystic-->
        <Property>
            <name>Aileron</name>
            <columNumber>0</columNumber>
            <minRange>-1</minRange>
            <maxRange>1</maxRange>
        </Property>

        <Property>
            <name>Elevator</name>
            <columNumber>1</columNumber>
            <minRange>-1</minRange>
            <maxRange>1</maxRange>
        </Property>

        <Property>
            <name>Rudder</name>
            <columNumber>2</columNumber>
            <minRange>-1</minRange>
            <maxRange>1</maxRange>
        </Property>

        <Property>
            <name>Throttle</name>
